#include<stdio.h>
#include<bits/stdc++.h>
#include<string>
#include<stdlib.h>
#include<sstream>
#include<iostream>
#include<vector>
#include<map>
#include<utility>//pair
#include<iterator>
#define ll long long
#define ull unsigned long long
//typedef ll int pp;
//typedef ll int qq;
//typedef map<string,int> msi;
//typedef map<int,int>mii;
//#define itrci map<char,int>::iterator
#define mod 1000000007
#define pi acos(-1)
#define zero 0
#define pb push_back
#define FASTIO ios::sync_with_stdio(false);cin.tie(0);
using namespace std;
 
int main()
{
    int n,m,k;
    cin>>n>>m>>k;
    int a[n];
    int x=0;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        if(a[i]>m){x++;}
    }
    sort(a,a+n);
    int mx=a[n-1];
    if(mx<m)
    {
        if(k>=1)
        {
            cout<<"YES"<<endl;
        }
        else if(k==0)
        {
            cout<<"NO"<<endl;
        }
    }
    else
    {
        if(x<=k)
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }
    
 
    return 0;
}
